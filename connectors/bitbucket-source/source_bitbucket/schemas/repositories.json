{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "description": "Object type (repository)"
    },
    "full_name": {
      "type": "string",
      "description": "Full repository name (workspace/repo_name)"
    },
    "links": {
      "type": "object",
      "properties": {
        "self": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          }
        },
        "html": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          }
        },
        "avatar": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          }
        },
        "pullrequests": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          }
        },
        "commits": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          }
        },
        "forks": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          }
        },
        "watchers": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          }
        },
        "branches": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          }
        },
        "tags": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          }
        },
        "downloads": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          }
        },
        "source": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          }
        },
        "clone": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "href": {
                "type": "string"
              }
            }
          }
        },
        "hooks": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          }
        }
      }
    },
    "name": {
      "type": "string",
      "description": "Repository name"
    },
    "slug": {
      "type": "string",
      "description": "Repository slug"
    },
    "description": {
      "type": ["string", "null"],
      "description": "Repository description"
    },
    "scm": {
      "type": "string",
      "description": "Source control management type (git, hg)"
    },
    "website": {
      "type": ["string", "null"],
      "description": "Repository website URL"
    },
    "owner": {
      "type": "object",
      "properties": {
        "display_name": {
          "type": "string"
        },
        "links": {
          "type": "object",
          "properties": {
            "self": {
              "type": "object",
              "properties": {
                "href": {
                  "type": "string"
                }
              }
            },
            "avatar": {
              "type": "object",
              "properties": {
                "href": {
                  "type": "string"
                }
              }
            },
            "html": {
              "type": "object",
              "properties": {
                "href": {
                  "type": "string"
                }
              }
            }
          }
        },
        "type": {
          "type": "string"
        },
        "uuid": {
          "type": "string"
        },
        "username": {
          "type": "string"
        }
      }
    },
    "workspace": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string"
        },
        "uuid": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "slug": {
          "type": "string"
        },
        "links": {
          "type": "object",
          "properties": {
            "avatar": {
              "type": "object",
              "properties": {
                "href": {
                  "type": "string"
                }
              }
            },
            "html": {
              "type": "object",
              "properties": {
                "href": {
                  "type": "string"
                }
              }
            },
            "self": {
              "type": "object",
              "properties": {
                "href": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "is_private": {
      "type": "boolean",
      "description": "Whether the repository is private"
    },
    "project": {
      "type": ["object", "null"],
      "properties": {
        "type": {
          "type": "string"
        },
        "key": {
          "type": "string"
        },
        "uuid": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "links": {
          "type": "object",
          "properties": {
            "self": {
              "type": "object",
              "properties": {
                "href": {
                  "type": "string"
                }
              }
            },
            "html": {
              "type": "object",
              "properties": {
                "href": {
                  "type": "string"
                }
              }
            },
            "avatar": {
              "type": "object",
              "properties": {
                "href": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "fork_policy": {
      "type": "string",
      "description": "Fork policy"
    },
    "created_on": {
      "type": "string",
      "format": "date-time",
      "description": "Creation timestamp"
    },
    "updated_on": {
      "type": "string",
      "format": "date-time",
      "description": "Last update timestamp"
    },
    "size": {
      "type": "integer",
      "description": "Repository size in bytes"
    },
    "language": {
      "type": ["string", "null"],
      "description": "Primary programming language"
    },
    "uuid": {
      "type": "string",
      "description": "Repository UUID"
    },
    "mainbranch": {
      "type": ["object", "null"],
      "properties": {
        "name": {
          "type": "string"
        },
        "type": {
          "type": "string"
        }
      }
    },
    "override_settings": {
      "type": "object",
      "properties": {
        "default_merge_strategy": {
          "type": "boolean"
        },
        "branching_model": {
          "type": "boolean"
        }
      }
    },
    "parent": {
      "type": ["object", "null"],
      "description": "Parent repository if this is a fork"
    },
    "enforced_signed_commits": {
      "type": ["boolean", "null"],
      "description": "Whether signed commits are enforced"
    },
    "has_issues": {
      "type": "boolean",
      "description": "Whether issues are enabled"
    },
    "has_wiki": {
      "type": "boolean",
      "description": "Whether wiki is enabled"
    }
  }
}
